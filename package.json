{
  "name": "ramelix",
  "version": "1.0.0",
  "description": "RAML configuration for API REST ",
  "main": "./bin/www",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "start": "node ./bin/www",
    "lint": "xo ./routes/*.js ./bin/www",
    "html": "raml2html ./api/ramelix.raml > ./public/index.html",
    "server": "serve ./public/ -p 8000"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "raml",
    "express",
    "osprey",
    "node"
  ],
  "author": "jansanchez",
  "license": "MIT",
  "homepage": "",
  "dependencies": {
    "chalk": "^1.1.3",
    "express": "^4.13.4",
    "osprey": "^0.3.1",
    "raml2html": "^2.4.0"
  },
  "xo": {
    "esnext": true,
    "rules": {
      "babel/new-cap": 0
    }
  },
  "devDependencies": {
    "serve": "^1.4.0",
    "watch": "^0.17.1",
    "xo": "^0.14.0"
  }
}
